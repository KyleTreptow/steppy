{
    "panels":[
       {
          "title":"Personal Information",
          "inputs":[
             {
                "type":"TextControl",
                "label":"First Name",
                "inputName":"first_name",
                "placeholder":"John",
                "width": 1
             },
             {
                "type":"TextControl",
                "label":"Last Name",
                "inputName":"last_name",
                "placeholder":"Doe",
                "width": 1
             },
             {
                "type":"EmailControl",
                "label":"Email",
                "inputName":"email",
                "placeholder":"john@doe.com",
                "width": 2
             }
          ]
       },
       {
          "title":"Vehicle & Education",
          "inputs":[
             {
                "type":"RadioControl",
                "label":"Car Make",
                "inputName":"car_name",
                "options":[
                   "Toyota",
                   "Ford",
                   "Chevy"
                ],
                "width": 1
             },
             {
                "type":"RadioControl",
                "label":"School",
                "inputName":"school",
                "options":[
                   "CSUN",
                   "USC",
                   "UCLA",
                   "Other"
                ],
                "width": 1
             }
          ]
       },
       {
          "title":"Additional Information",
          "inputs":[
             {
                "type":"CheckboxControl",
                "label":"Colors",
                "inputName":"colors",
                "options":[
                   "Red",
                   "Blue",
                   "Green"
                ],
                "width": 1
             },
             {
                "type":"TextControl",
                "label":"Nickname",
                "inputName":"nickname",
                "placeholder":"Chester",
                "width": 1
             }
          ]
       },
       {
          "title":"Geometry",
          "inputs":[
             {
                "type":"SelectControl",
                "label":"Shape",
                "inputName":"shape",
                "options":[
                   "Square",
                   "Circle",
                   "Triangle"
                ],
                "width": 1
             }
          ]
       }
    ]
 }
